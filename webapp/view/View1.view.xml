<mvc:View controllerName="scrollcontainer.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:u="sap.ui.unified">
    <Page id="page" title="{i18n>title}">
        <Bar >
            <contentRight>
                <Button text="Excel Upload" press="onUploadDialog" />
                <Button text="Uri" press="onUrl" />
            </contentRight>
        </Bar>
        <ScrollContainer >
        <Table id="tableInfo" fixedLayout="false"></Table>
        </ScrollContainer>
    </Page>
</mvc:View>