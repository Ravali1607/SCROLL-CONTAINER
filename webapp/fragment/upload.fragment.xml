            class="sapUiTinyMargin"
            id="fileUploader"
            change="fileUpload"
            
            ></unified:FileUploader>
            <footer>
                <Toolbar >
                    <ToolbarSpacer ></ToolbarSpacer>
                    <Button text="Upload" press="uploadExcel"></Button>
                </Toolbar>
            </footer>
    </Dialog>
</core:FragmentDefinition>